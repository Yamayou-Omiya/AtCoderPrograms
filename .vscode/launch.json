{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "A01 .NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build A01",
            "program": "${workspaceFolder}/A01/bin/Debug/net8.0/A01.dll",
            "args": [],
            "cwd": "${workspaceFolder}/A01",
            "stopAtEntry": false,
            "console": "internalConsole",
            "internalConsoleOptions": "openOnSessionStart",
            "launchBrowser": {
                "enabled": false
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/Views"
            }
        }
    ]
}
