{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "A .NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build A",
            "program": "${workspaceFolder}/A/bin/Debug/net8.0/A.dll",
            "args": [],
            "cwd": "${workspaceFolder}/A",
            "stopAtEntry": false,
            "console": "internalConsole",
            "internalConsoleOptions": "openOnSessionStart",
            "launchBrowser": {
                "enabled": false
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/Views"
            }
        },
